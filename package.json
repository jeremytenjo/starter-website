{
  "name": "website-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "ts-node-esm --project ./devtools/utils/terminal/terminal-tsconfig.json ./scripts/dev/run.ts --dataSource=dev",
    "dev:signedIn": "ts-node-esm --project ./devtools/utils/terminal/terminal-tsconfig.json ./scripts/dev/run.ts --signedIn --dataSource=dev",
    "dev:dataSource-prod": "ts-node-esm --project ./devtools/utils/terminal/terminal-tsconfig.json ./scripts/dev/run.ts --dataSource=prod",
    "dev:onlyApp": "ts-node-esm --project ./devtools/utils/terminal/terminal-tsconfig.json ./scripts/dev/run.ts --onlyApp",
    "dev:for-testing": "ts-node-esm --project ./devtools/utils/terminal/terminal-tsconfig.json ./scripts/dev/run.ts --dataSource=prod --onlyApp",
    "dev:nextjs": "next dev -p 3001",
    "storybook:dev": "node --experimental-json-modules --loader ts-node/esm ./scripts/storybook/startStorybook/run.ts",
    "storybook:test:run": "node --experimental-json-modules --loader ts-node/esm ./scripts/storybook/runStorybookTests/run.ts",
    "storybook:test:watch": "node --experimental-json-modules --loader ts-node/esm ./scripts/storybook/runStorybookTestsWatch/run.ts",
    "functions:dev": "node --experimental-json-modules --loader ts-node/esm ./firebaseFunctions/scripts/buildFunctions/run.ts --watch",
    "functions:build": "node --experimental-json-modules --loader ts-node/esm ./firebaseFunctions/scripts/buildFunctions/run.ts",
    "functions:deploy": "node --experimental-json-modules --loader ts-node/esm ./firebaseFunctions/scripts/deployFunctions/run.ts",
    "cms:slicemachine": "ts-node-esm ./scripts/cms/prismic/startSliceMachine/run.ts",
    "cms:prismic-generate-types": "prismic-ts-codegen -c ./src/lib/integrations/Prismic/codegen/prismic.codegen.config.ts",
    "build": "node --experimental-json-modules --loader ts-node/esm ./scripts/buildApp/run.ts",
    "build:analyze": "ANALYZE=true next build",
    "build-and-serve": "npm run build && npm run serve",
    "serve": "next start",
    "tests:playwright:ui": "node --experimental-json-modules --loader ts-node/esm ./devtools/testing/playwright/scripts/runPlayWright/run.ts --ui",
    "tests:playwright:watch": "node --experimental-json-modules --loader ts-node/esm ./devtools/testing/playwright/scripts/runPlayWright/run.ts --watch",
    "tests:playwright:single:test": "node --experimental-json-modules --loader ts-node/esm ./devtools/testing/playwright/scripts/runPlayWright/run.ts --singleTest",
    "tests:playwright:run:all": "node --experimental-json-modules --loader ts-node/esm ./devtools/testing/playwright/scripts/runPlayWright/run.ts",
    "tests:vitest": "vitest --config ./devtools/testing/vitest/vitest.config.ts",
    "tests:vitest:ci": "vitest --config ./devtools/testing/vitest/vitest.config.ts --run --passWithNoTests",
    "tests:lint": "eslint ./ --fix",
    "tests:ts-types": "tsc --noemit",
    "tests:all": "COMMIT_SITEMAP=true npm run tests:ts-types && npm run tests:lint && npm run tests:vitest:ci && npm run build",
    "prettify": "node --loader  ts-node/esm ./devtools/prettier/scripts/runPrettier/runPrettier.ts",
    "prepare": "node --experimental-json-modules --loader ts-node/esm ./scripts/prepare/run.ts",
    "helpers:generate-sitemap": "npm run build && next-sitemap --config next-sitemap.js",
    "helpers:generate-sitemap-and-push": "node --loader ts-node/esm ././scripts/sitemap/genAndPushSitemap/run.ts",
    "helpers:generate-logo-assets": "node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/assets/generateLogoAssets/run.ts",
    "helpers:create-icon": "node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/createIcon/run.ts",
    "helpers:capture-page-screenshots": "node --loader  ts-node/esm ./scripts/assets/capturePageScreenshots/run.ts",
    "helpers:update-useweb-deps": "npm-check-updates -f /@useweb/*/ -u && npm i && npm run prettify && npm run tests:ts-types",
    "helpers:generate-app-map": "node --experimental-json-modules --loader ts-node/esm ./devtools/codeGenerator/appGenerator/generateAppMap/run.ts && npm run prettify",
    "helpers:figma-to-code": "node --experimental-json-modules --loader ts-node/esm node_modules/.bin/figma-to-code -c ./devtools/figmaToCode/figma-to-code.config.ts && npm run tests:ts-types",
    "helpers:figma-to-code:fromFigmaUrl": "node --experimental-json-modules --loader ts-node/esm node_modules/.bin/figma-to-code --enquireFigmaUrl -c ./devtools/figmaToCode/figma-to-code.config.ts && npm run tests:ts-types",
    "helpers:figma-to-code:getFigmaNodeScreenshot": "node --experimental-json-modules --loader ts-node/esm node_modules/.bin/figma-to-code --getFigmaNodeScreenshot -c ./devtools/figmaToCode/figma-to-code.config.ts && npm run tests:ts-types",
    "helpers:figma-to-code:enquire": "node --experimental-json-modules --loader ts-node/esm node_modules/.bin/figma-to-code --enquire -c ./devtools/figmaToCode/figma-to-code.config.ts && npm run tests:ts-types",
    "helpers:remove-empty-folders": "node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/files/removeEmptyFoldersInSrc/run.ts",
    "helpers:generate-blurred-public-images-copies": "node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/assets/generateBlurredPublicImagesCopies/run.ts"
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.5",
    "@mui/material": "5.10.1",
    "@next/font": "13.1.1",
    "@prismicio/client": "6.6.4",
    "@prismicio/helpers": "2.3.3",
    "@prismicio/next": "1.0.2",
    "@prismicio/react": "2.4.4",
    "@prismicio/slice-simulator-react": "0.2.2",
    "@useweb/assert": "2.0.0",
    "@useweb/async-tester": "3.2.1",
    "@useweb/click-to-component": "2.6.4",
    "@useweb/fetch-tester": "1.31.0",
    "@useweb/fetching-ui": "7.0.5",
    "@useweb/figma-to-code": "2.29.1",
    "@useweb/firebase": "8.29.0",
    "@useweb/firebase-function-tester": "6.0.0",
    "@useweb/object-to-array": "2.0.0",
    "@useweb/pixel-perfect": "6.0.6",
    "@useweb/task": "2.0.1",
    "@useweb/ui": "7.15.0",
    "@useweb/ui-theme": "2.76.1",
    "@useweb/use-async": "7.6.8",
    "@useweb/use-async-ui": "1.5.0",
    "@useweb/use-data": "20.9.1",
    "@useweb/use-data-ui": "1.8.0",
    "@useweb/use-event-listener": "1.4.0",
    "@useweb/use-fetch": "1.6.0",
    "@useweb/use-next-api": "1.3.0",
    "emotion-theming": "11.0.0",
    "figma-api": "1.10.1",
    "firebase": "9.22.0",
    "firebase-admin": "11.8.0",
    "json-to-ts": "1.7.0",
    "next": "13.4.8",
    "react": "18.2.0",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-hook-form": "7.34.2",
    "react-json-view": "1.21.3",
    "swr": "2.0.4",
    "zustand": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@next/bundle-analyzer": "13.0.6",
    "@playwright/test": "1.35.1",
    "@prismicio/types": "0.2.3",
    "@slicemachine/adapter-next": "0.2.1",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/addon-docs": "7.0.26",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/blocks": "7.0.26",
    "@storybook/builder-webpack5": "7.0.26",
    "@storybook/components": "7.0.26",
    "@storybook/jest": "0.1.0",
    "@storybook/nextjs": "7.0.26",
    "@storybook/test-runner": "0.11.0",
    "@storybook/testing-library": "0.2.0",
    "@svgr/core": "6.3.1",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@useweb/starter-website-code-gen-templates": "7.29.0",
    "@vitejs/plugin-react": "2.0.1",
    "babel-loader": "8.2.5",
    "capture-website": "3.0.0",
    "chalk": "5.0.1",
    "change-case": "4.1.2",
    "clipboardy": "3.0.0",
