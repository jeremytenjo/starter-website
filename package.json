{
  "name": "website-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --experimental-json-modules --loader ts-node/esm ./scripts/dev/run.ts",
    "dev:dataSource-prod": "node --experimental-json-modules --loader ts-node/esm ./scripts/dev/run.ts --dataSource=prod",
    "dev:for-testing": "node --experimental-json-modules --loader ts-node/esm ./scripts/dev/run.ts --dataSource=prod --onlyApp",
    "ladle:dev": "ladle serve --config ./devtools/ladle --open none",
    "ladle:build": "ladle build --config ./devtools/ladle",
    "build": "next build",
    "serve": "next start",
    "build-and-serve": "npm run build && npm run serve",
    "tests:e2e:watch": "node --experimental-json-modules --loader ts-node/esm ./devtools/testing/playwright/scripts/runPlayWright/run.ts --watch",
    "tests:e2e:run:all": "node --experimental-json-modules --loader ts-node/esm ./devtools/testing/playwright/scripts/runPlayWright/run.ts",
    "tests:unit": "vitest --config ./devtools/testing/vitest/vitest.config.ts",
    "tests:unit:ci": "vitest --config ./devtools/testing/vitest/vitest.config.ts --run --passWithNoTests",
    "tests:eslint": "eslint ./ --fix",
    "tests:ts-types": "tsc --noemit",
    "prettify": "node --loader  ts-node/esm ./devtools/prettier/scripts/runPrettier/runPrettier.ts",
    "helpers:generate-logo-assets": "node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/assets/generateLogoAssets/run.ts",
    "helpers:create-icon": "node --experimental-json-modules --loader ts-node/esm ./devtools/helpers/createIcon/run.ts",
    "helpers:capture-page-screenshots": "node --loader  ts-node/esm ./scripts/assets/capturePageScreenshots/run.ts",
    "helpers:update-useweb-deps": "npm-check-updates -f /@useweb/*/ -u && npm i",
    "prepare": "npx @arkweid/lefthook install",
    "postbuild": "next-sitemap --config next-sitemap.cjs"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@ladle/react": "^0.12.1",
    "@mui/material": "^5.3.1",
    "@playwright/test": "^1.21.1",
    "@useweb/click-to-component": "^1.0.5",
    "emotion-theming": "^11.0.0",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@svgr/core": "^6.2.1",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@vitejs/plugin-react": "^1.3.2",
    "capture-website": "^2.3.0",
    "chalk": "^4.1.2",
    "clipboardy": "^3.0.0",
    "command-line-args": "^5.2.0",
    "concurrently": "^7.0.0",
    "edit-json-file": "^1.7.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.9",
    "fs-extra": "^10.0.0",
    "ink": "^3.2.0",
    "internal-ip": "^7.0.0",
    "jsdom": "^19.0.0",
    "kill-port-process": "^3.0.1",
    "next-sitemap": "^1.9.12",
    "node-emoji": "^1.11.0",
    "node-watch": "^0.7.3",
    "ora": "^6.0.1",
    "prettier": "^2.5.1",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.29.3",
    "svgo": "^2.8.0",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vitest": "^0.10.2"
  }
}
